{
    "name": "webignition/web-resource-retriever",
    "description": "Retrieve a WebResourceInterface instance over HTTP",
    "keywords": ["web", "resource", "retriever", "web-page", "json-document", "psr-7"],
    "homepage": "https://github.com/webignition/web-resource-retriever",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\WebResource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\Tests\\WebResource\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always",
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR2",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "webignition/web-resource-interfaces":"^0.9",
        "webignition/internet-media-type-interface":"^0.1",
        "webignition/web-resource-model": "^3",
        "webignition/web-page-model": "^3",
        "webignition/web-json-model": "^3",
        "webignition/internet-media-type":"^1.2",
        "guzzlehttp/guzzle":"^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^3",
        "mockery/mockery": "^1.0",
        "webignition/web-resource-testing-tools":"^0.2"
    },
    "minimum-stability":"stable"
}